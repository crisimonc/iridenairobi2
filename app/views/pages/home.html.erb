<!-- date picker -->
<div class="date-picker">
<%= simple_form_for :search, url: motorcycles_path, method: "get", wrapper: :inline_form, html: { class: "form-inline" }, wrapper_mappings: {boolean: :inline_boolean} do |f| %>
	<%= f.input :starts_at, as: :string, placeholder: 'Start Date', required: false, input_html: {id: "start_date", class: "datepicker mr-sm-2"} %>
	<%= f.input :ends_at, as: :string, placeholder: 'End Date', required: false, input_html: {id: "end_date", class: "datepicker mr-sm-2"} %>
	<%= f.button :submit, "Search", class: "btn-primary " %>
<% end %>
</div>

